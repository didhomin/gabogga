<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">
	<bean id="ds" class="org.springframework.jndi.JndiObjectFactoryBean">
		<property name="jndiName" value="java:comp/env/jdbc/kitri"/>
	</bean>
	<bean id="smc" class="org.springframework.orm.ibatis.SqlMapClientFactoryBean">
		<property name="configLocation" value="classpath:com/gbg/config/sqlmap/SqlMapConfig.xml"/>
		<property name="dataSource" ref="ds"/>
	</bean>
	
	<bean id="ms" class="com.gbg.member.service.MemberServiceImpl">
		<property name="memberDao" ref="md"/>
	</bean>
	
	<bean id="md" class="com.gbg.member.dao.MemberDaoImpl">
		<property name="sqlMapClient" ref="smc"/>
	</bean>
	
	
	
	
	
</beans>
