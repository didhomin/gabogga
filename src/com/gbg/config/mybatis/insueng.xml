<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.gbg.memberAdmin.dao.MemberAdminDao">
      <delete id="memberAdmindelete" parameterType="String" >
     delete users
     where user_Id=#{userId}
    </delete>
    
    <select id="listMemberAdmin" resultType="com.gbg.member.model.UsersDto">
    select tel ,user_id userId,email from users
    </select>
    
    <select id="searchMemberAdmin" parameterType="String" resultType="com.gbg.member.model.UsersDto">
     select tel , user_Id, email,gender,reg_Date regDate from users
     where user_Id=#{id}
    </select>
    
</mapper>