<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="rg">
    <select id="idcount" parameterClass="String" resultClass="int">
        select count(id)
        from member
        where id=#id#
    </select>
    
   
</sqlMap>